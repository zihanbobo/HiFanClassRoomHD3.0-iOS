platform :ios, '8.0'
target 'HiFanClassRoomHD' do

inhibit_all_warnings!                          #可能产生其他问题

    pod 'Masonry'
    pod 'MJRefresh'
    pod 'MBProgressHUD'
    pod 'JPush'
    
    use_frameworks!
    pod 'ReactiveCocoa', '~> 2.5.0'            # 必须加上上面的use_frameworks!
    
    pod 'AFNetworking'
    pod 'SDWebImage'
    pod 'YYModel'
    pod 'WMPlayer'                             #视频播放
    
    pod 'Bugly'
    pod 'IQKeyboardManager'
    pod 'FLAnimatedImage'
                                               # 友盟 U-Share SDK
    pod 'UMengUShare/Social/ReducedWeChat'     # 集成微信(精简版0.2M)
    pod 'UMengUShare/Social/QQ'                # 集成QQ(完整版7.6M)
    pod 'UMengUShare/Social/ReducedSina'       # 集成新浪微博(精简版1M)
    pod 'UMengAnalytics-NO-IDFA'               #友盟统计-无IDFA版SDK
    
    
end


